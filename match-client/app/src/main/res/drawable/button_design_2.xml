<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#22cc14" />
            <gradient android:angle="-90"
                android:startColor="#22cc14"
                android:endColor="#29ca29"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#29ca29" />
            <solid android:color="#29ca29"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#26ab26" />
            <gradient android:angle="-90" android:startColor="#29ca29" android:endColor="#58857e" />
        </shape>
    </item>
</selector>