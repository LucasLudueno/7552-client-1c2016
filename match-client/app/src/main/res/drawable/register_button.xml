<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <stroke android:width="1dip" android:color="#c18b62" />
            <gradient android:angle="-90"
                android:startColor="#f7a76a"
                android:endColor="#f7a76a"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  > />
            <stroke android:width="1dip" android:color="#FFD69666" />
            <solid android:color="#a97650"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  > />
            <stroke android:width="1dip" android:color="#FFD69666" />
            <gradient android:angle="-90" android:startColor="#FFD69666" android:endColor="#FFD69666" />
        </shape>
    </item>
</selector>